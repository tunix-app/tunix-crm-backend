{
  "version": 2,
  "builds": [
    { 
      "src": "api/index.ts",
      "use": "@vercel/node" 
    }
  ],
  "routes": [
    { 
      "src": "/(.*)", 
      "dest": "api/index.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    }
  ],
  "github": {
    "productionBranch": "main",
    "silent": true
  }
}